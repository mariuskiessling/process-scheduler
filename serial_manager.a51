$NOMOD51
#include <reg517a.inc>

; ------------------------------------------------------------------------------
; Module name
; ------------------------------------------------------------------------------
Name SerialManager


; ------------------------------------------------------------------------------
; Published symbols
; ------------------------------------------------------------------------------
PUBLIC printToSerial


; ------------------------------------------------------------------------------
; Code segement definition
; ------------------------------------------------------------------------------
serialManagerCode SEGMENT CODE
				  RSEG serialManagerCode


; ================================ Module code =================================

; ------------------------------------------------------------------------------
; Prints the content of A to a serial output
; ------------------------------------------------------------------------------
printToSerial:
	CLR TI0
	MOV S0BUF, A
	JNB TI0,$
	RET

END
